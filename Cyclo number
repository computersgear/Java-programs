import java.util.*;
class cyclo
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int c=0,d=0;
        System.out.println("Enter a number");
        int n = sc.nextInt();
        c=n%10;
        while(n!=0)
        {
             d=n%10;
             n=n/10;
        }
        if(c==d)
        System.out.println("Cyclo number");
        else
        System.out.println("Not a Cyclo number");
    }
}
